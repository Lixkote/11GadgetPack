<UserControl xmlns:my="using:Microsoft.UI.Xaml.Controls"  x:Class="ShellApp.Shell.Start.SettingsXAML"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ShellApp.Shell.Start"
             xmlns:data="using:System.Linq.Expressions"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" Height="800" Width="550">

    <UserControl.Resources>

    </UserControl.Resources>



    <my:NavigationView x:Name="Settingsnav" IsBackButtonVisible="Collapsed" IsSettingsVisible="False" PaneDisplayMode="Top" SelectionChanged="Settingsnav_SelectionChanged">
        <my:NavigationView.MenuItems>
            <my:NavigationViewItem  Content="Welcome" Tag="Welcome" />
            <my:NavigationViewItem  Content="Themes" Tag="Themes" />
            <my:NavigationViewItem  Content="Tools" Tag="Tools" />
            <my:NavigationViewItem  Content="About" Tag="About" />
        </my:NavigationView.MenuItems>
        <Frame x:Name="contentFrame" CornerRadius="10,10,10,10"/>
    </my:NavigationView>

</UserControl>
